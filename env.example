# Firebase Configuration
# Copy this file to .env and fill in your actual values from Firebase Console
# Never commit .env file to the repository

VITE_FIREBASE_API_KEY=your_firebase_api_key_here
VITE_FIREBASE_AUTH_DOMAIN=your-project.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your-project-id
VITE_FIREBASE_STORAGE_BUCKET=your-project.firebasestorage.app
VITE_FIREBASE_MESSAGING_SENDER_ID=123456789012
VITE_FIREBASE_APP_ID=1:123456789012:web:abcdef1234567890
VITE_FIREBASE_MEASUREMENT_ID=G-XXXXXXXXXX

# Google Translate API (Optional - for translation features)
VITE_GOOGLE_TRANSLATE_API_KEY=your_google_translate_api_key_here

# DeepL API (Optional - for translation features)
VITE_DEEPL_API_KEY=your_deepl_api_key_here
